        function delete_cart(nid) {
            var r = confirm("Bạn có muốn xóa hay không?");
            if (r) {
                var dataString = "nid=" + nid;
                $.ajax({
                    type: "POST",
                    url: "https://hieuhaisan.com/delete_cart",
